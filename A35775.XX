;GENERATES A 2K X 8 FILE FOR 2716
;SWITCH COMBINATION 6

	.ENABL M68
	.NLIST BYT
	.MACRO .PIC,A,B,C,D,E,F,G,H
	.LIST MEB
	.WORD 0'A,0'B,0'C,0'D,0'E,0'F,0'G,0'H
	.NLIST MEB
	.ENDM
	.MACRO .GRA, A,B,C,D,E,F,G,H,I,J,K,L,M,N,O,P
	.LIST MEB
	.BYTE ^C0'A,^C0'B,^C0'C,^C0'D,^C0'E,^C0'F,^C0'G,^C0'H
	.BYTE ^C0'I,^C0'J,^C0'K,^C0'L,^C0'M,^C0'N,^C0'O,^C0'P
	.NLIST MEB
	.ENDM
FLAGA	=1			;0=INVISIBLE ARROWS, 1=ARROWS VISIBLE

	.LIST ME
	.MACRO ARW H,I,J,K,L,M,N,O,P,Q,R,S,T,U,V,W
	.IF NE,FLAGA		;VISIBLE ARROWS
	.GRA H,I,J,K,L,M,N,O,P,Q,R,S,T,U,V,W
	.ENDC
	.IF EQ,FLAGA		;INVISIBLE ARROWS
	.BYTE 0FF,0FF,0FF,0FF,0FF,0FF,0FF,0FF,0FF,0FF,0FF,0FF,0FF,0FF,0FF,0FF
	.ENDC
	.ENDM
	.MACRO COMP ..X		;TAKE COMPLEMENT OF ..X
	.WORD ^C ..X
	.ENDM
	.MACRO IREV ..X		;REVERSE BITS OF ..X
	.NLIST ME
	A=..X&1/1		;FIRST ISOLATE EACH BIT IN D0
	B=..X&2/2
	C=..X&4/4
	D=..X&8/8
	E=..X&10/10
	F=..X&20/20
	G=..X&40/40
	H=..X&80/80
	I=..X&100/100
	J=..X&200/200
	K=..X&400/400
	L=..X&800/800
	M=..X&1000/1000
	N=..X&2000/2000
	O=..X&4000/4000
	P=<<..X/4000>&2>/2	;NEXT RECOMBINE IN REVERSE
ANS1	=<A*8000>!<B*4000>!<C*2000>!<D*1000>!<E*800>!<F*400>!<G*200>!<H*100>
ANS2	=<I*80>!<J*40>!<K*20>!<L*10>!<M*8>!<N*4>!<O*2>!<P>
ANS	=ANS1!ANS2
	.LIST ME
	.ENDM
	.MACRO REV ..X
	IREV ..X
	.WORD ANS
	.ENDM
	.MACRO BOTH ..X		;TAKE COMPL & REVERSE
	..Y=^C ..X
	IREV ..Y
	.WORD ANS
	.ENDM


